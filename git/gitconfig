[user]
	email = xianmin12@qq.com
	name = Xianmin Chen
[push]
    default = matching
[core]
	editor = code --wait
    excludesfile = ~/.gitignore

# 参考：https://gist.github.com/pksunkara/988716

[diff]
    tool = default-difftool
[difftool "default-difftool"]
    cmd = code --wait --diff $LOCAL $REMOTE